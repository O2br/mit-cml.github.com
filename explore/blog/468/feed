          &lt;figure class=&quot;field-item&quot;&gt;
        &lt;div id=&quot;file-21906&quot; class=&quot;file file-image file-image-jpeg&quot;&gt;

        &lt;h2 class=&quot;element-invisible&quot;&gt;&lt;a href=&quot;/file/teen-code4jpg&quot;&gt;Teen-Code4.jpg&lt;/a&gt;&lt;/h2&gt;
    
  
