          &lt;figure class=&quot;field-item&quot;&gt;
        &lt;div id=&quot;file-21963&quot; class=&quot;file file-image file-image-jpeg&quot;&gt;

        &lt;h2 class=&quot;element-invisible&quot;&gt;&lt;a href=&quot;/file/girls-show-appjpg&quot;&gt;girls-show-off-app.jpg&lt;/a&gt;&lt;/h2&gt;
    
  
  &lt;div class=&quot;content&quot;&gt;
    &lt;img typeof=&quot;foaf:Image&quot; class=&quot;image-style-none&quot; src=&quot;http://explore.appinventor.mit.edu/sites/explore.appinventor.mit.edu/files/blog/girls-show-off-app.jpg&quot; width=&quot;800&quot; height=&quot;533&quot; alt=&quot;&quot; /&gt;  &lt;/div&gt;

  
&lt;/div&gt;
              &lt;/figure&gt;
